<div class="modal-header">
    <h4>{{title}}</h4>
</div>
<div class="modal-body">
    <div style="text-align:justify" ng-bind-html="text"></div>
</div>
<div ng-if="msg" class="modal-footer" style="text-align:left">
    <p ng-repeat="i in msg" >
        <a href="#" ng-click="action(i, $event)">{{i.options}}</a> 
    </p> 
</div>
